{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "userRoutes.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "build": {
      "env": {
        "CUSTOM_KEY": "value"
      }
    }
  }
  